    "seed-production": "node src/db/seeds/seed-firestore.js",
    "postseed-production": "firebase deploy --only firestore:rules --config firestore.rules.json"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "@stripe/react-stripe-js": "^3.1.1",
    "@stripe/stripe-js": "^5.5.0",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "firebase": "^11.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.28.2",
    "react-slick": "^0.30.3",
    "redux-persist": "^6.0.0",
    "stripe": "^17.5.0",
    "styled-components": "^6.1.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "vite": "^6.0.5"
